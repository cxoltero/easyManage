<div class="container">
  <div class="options">
    <%= link_to "New Note", new_note_path %>
  </div>
<%@notes.each do|note| %>
	<h2><%=link_to note.title, note%></h2>
	<p><%= time_ago_in_words(note.created_at)%></p>
<%end%>
<%= link_to "All Notes", notes_path(@note), class: "button" %>
<%= link_to "Back", root_path, class: "btn btn-default" %>
</div>