<div class="container">
	<div class="incomename">
	<tr>
	<td><%= income.amount %></td>
	<td><%= income.from %></td>
	<td><%= income.date %></td>
	<td><center><%= link_to "Delete", [income.month, income], method: :delete, data: {confirm: "Are you sure you want to delete this income?"}, class: "btn btn-danger" %></center></td>
	</tr>
	<p><%= time_ago_in_words(income.created_at) %> Ago</p>
	</div>
</div>