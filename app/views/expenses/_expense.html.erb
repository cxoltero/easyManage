<div class="container">
	<div class="expensename">
	<%= expense.amount %>
	<%= expense.to %>
	<%= expense.duedate %></span>
	<%= link_to "Delete", [expense.month, expense], method: :delete, data: {confirm: "Are you sure you want to delete this expense?"}, class: "btn btn-danger" %>
	<p><%= time_ago_in_words(expense.created_at) %> Ago</p>
	</div>
</div>