<div class="container">
  <div class="options">
    <%= link_to "Add List", new_tasklist_path %>
  </div>
  <div class="tasklist">
    <% @tasklists.each_slice(3) do |tasklists| %>
      <div class="row">
        <% tasklists.each do |tasklist| %>
          <div class=".col-md-4" id = "a1">
              <h2><span class="glyphicon glyphicon-check "></span><%= link_to tasklist.title, tasklist %></h2>
    	        <h6>Created on <%= tasklist.created_at.strftime("%B, %d, %Y") %></h6>
          </div>
        <%end%>
      </div>
    <% end %>
  </div>
</div>