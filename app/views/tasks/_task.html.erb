<div class="container">
	<div class="taskname">
	<%= task.name %>
	<%= task.duedate %></span>
	<%= link_to "Delete", [task.tasklist, task], method: :delete, data: {confirm: "Are you sure you want to delete this list?"}, class: "btn btn-danger" %>
	<p><%= time_ago_in_words(task.created_at) %> Ago</p>
	</div>
</div>